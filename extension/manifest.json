{
"manifest_version" : 2,
"description":"Github topics ranking power. Searching for repository topics rank and markup with power level like ",
"name": "TopGit",
"version" : "1.0.0",
"icons": { 
    "16" : "icon.png",
    "48" : "icon.png",
    "128": "icon.png" 
},
"content_scripts" : [
    {
    "matches": [
        "https://*.github.com/*"
    ],
    "js": ["topGit.js"]
    }
],
    "background":{
        "script" : ["background.js"]
    },
    "browser_action": {
        "default_icon": "icon.png"
    }
}